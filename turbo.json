{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "outputs": [
                "dist/**",
                ".next/**",
                "!.next/cache/**",
                "build/**"
            ],
            "dependsOn": [
                "^build"
            ]
        },
        "test": {
            "dependsOn": [
                "build"
            ],
            "outputs": [
                "coverage/**"
            ],
            "inputs": [
                "src/**/*.cpp",
                "src/**/*.h"
            ]
        },
        "lint": {},
        "dev": {
            "cache": false,
            "persistent": true
        },
        "clean": {
            "cache": false
        },
        "docs": {
            "outputs": [
                "docs/**"
            ],
            "inputs": [
                "src/**/*.h",
                "src/**/*.hpp",
                "src/**/*.cpp",
                "examples/**/*.cpp",
                "Doxyfile"
            ],
            "cache": true
        },
        "docs:clean": {
            "cache": false
        }
    }
}